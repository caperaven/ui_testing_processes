{
    "id": "framework pages",
    "main": {
        "steps": {
            "start": {
                "type": "perform",
                "action": "navigate",
                "args": {
                    "url": "${state.server}"
                },
                "next_step": "firstname_to_var"
            },
            "firstname_to_var": {
                "type": "system",
                "action": "properties_to_variables",
                "args": {
                    "#edtFirstName":{
                        "value": "$data.firstName"
                    }
                },
                "next_step": "check_firstname_var_value"
            },
            "check_firstname_var_value": {
                "type": "perform",
                "action": "type_text",
                "args": {
                    "query": "#paragraph",
                    "value": "$data.firstName"
                },
                "next_step": "wait_for_firstName_text"
            },
            "wait_for_firstName_text": {
                "type": "wait",
                "action": "time",
                "args": {
                    "timeout": 5
                },
                "next_step": "type_lastname"
            },
            "type_lastname": {
                "type": "perform",
                "action": "type_text",
                "args": {
                    "query": "#edtLastName",
                    "value": "Williams"
                },
                "next_step": "lastName_to_var"
            },
            "lastName_to_var": {
                "type": "system",
                "action": "properties_to_variables",
                "args": {
                    "#edtLastName": {
                        "value": "$data.lastName"
                    }
                },
                "next_step": "check_lastname_var_value"
            },
            "check_lastname_var_value": {
                "type": "perform",
                "action": "type_text",
                "args": {
                    "query": "#paragraph",
                    "value": "$data.lastName"
                },
                "next_step": "wait_lastname_text"
            },
            "wait_lastname_text": {
                "type": "wait",
                "action": "time",
                "args": {
                    "timeout": 5
                }
            }
        }
    }
}