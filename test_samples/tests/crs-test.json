{
    "id": "crs-test",
    "main": {
        "steps": {
            "start": {
                "type": "perform",
                "action": "navigate",
                "args": {
                    "url": "${state.server}"
                },
                "next_step": "random_int"
            },

            "random_int": {
                "type": "crs",
                "action": "call",
                "args": {
                    "type": "colors",
                    "action": "hex_to_rgb",
                    "args": {
                        "hex": "#ff0000"
                    },
                    "target": "$result.color"
                }
            }
        }
    }
}