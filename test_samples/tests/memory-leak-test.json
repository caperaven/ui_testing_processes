{
    "id": "assert-elements",
    "main": {
        "steps": {
            "start": {
                "type": "perform",
                "action": "navigate",
                "args": {
                    "url": "${state.server}"
                },
                "next_step": "set_uuid_variables"
            },

            "set_uuid_variables": {
                "type": "system",
                "action": "set_uuid_variables",
                "args": {
                    "variables": ["$data.firstName", "$data.lastName"]
                },
                "next_step": "type_value"
            }
        }
    }
}